{
    "@context": [
        "https://comp-rob2b.github.io/metamodels/geometry/coordinates.json",
        "https://comp-rob2b.github.io/metamodels/geometry/spatial-relations.json",
        "https://comp-rob2b.github.io/metamodels/qudt.json",
        {
            "sim-pickup": "https://hbrs-sesame.github.io/models/simulation/pickup/",

            "geom-sesame": "https://hbrs-sesame.github.io/metamodels/geometry#",
            "env-brsu": "https://hbrs-sesame.github.io/models/environments/brsu/",
            "env-avl": "https://hbrs-sesame.github.io/models/environment/avl/"
        }
    ],
    "@graph": [
        { "@id": "sim-pickup:world-frame-origin", "@type": "Point" },
        { "@id": "sim-pickup:world-frame", "@type": "Frame", "origin": "world-frame-origin" },

        {
            "@id": "sim-pickup:box-position", "@type": "Position",
            "of": "env-brsu:box-origin",
            "with-respect-to": "sim-pickup:world-frame-origin"
        },
        {
            "@id": "sim-pickup:box-orientation", "@type": "Orientation",
            "of": "env-brsu:box-frame",
            "with-respect-to": "sim-pickup:world-frame"
        },
        {
            "@id": "sim-pickup:box-pose", "@type": "Pose",
            "of": "env-brsu:box-frame",
            "with-respect-to": "sim-pickup:world-frame"
        },
        {
            "@id": "sim-pickup:box-pose-from-position-orientation",
            "@type": [
                "geom-sesame:PoseFromPositionOrientation", "PoseReference",
                "PositionReference", "OrientationReference", "PoseCoordinate"
            ],
            "of-pose": "sim-pickup:box-pose",
            "of-position": "sim-pickup:box-position",
            "of-orientation": "sim-pickup:box-orientation"
        },

        {
            "@id": "sim-pickup:ball-position", "@type": "Position",
            "of": "env-brsu:ball-origin",
            "with-respect-to": "sim-pickup:world-frame-origin"
        },
        {
            "@id": "sim-pickup:ball-orientation", "@type": "Orientation",
            "of": "env-brsu:ball-frame",
            "with-respect-to": "sim-pickup:world-frame"
        },
        {
            "@id": "sim-pickup:ball-pose", "@type": "Pose",
            "of": "env-brsu:ball-frame",
            "with-respect-to": "sim-pickup:world-frame"
        },
        {
            "@id": "sim-pickup:ball-pose-from-position-orientation",
            "@type": [
                "geom-sesame:PoseFromPositionOrientation", "PoseReference",
                "PositionReference", "OrientationReference", "PoseCoordinate"
            ],
            "of-pose": "sim-pickup:ball-pose",
            "of-position": "sim-pickup:ball-position",
            "of-orientation": "sim-pickup:ball-orientation"
        },

        {
            "@id": "sim-pickup:bottle-position", "@type": "Position",
            "of": "env-brsu:bottle-origin",
            "with-respect-to": "sim-pickup:world-frame-origin"
        },
        {
            "@id": "sim-pickup:bottle-orientation", "@type": "Orientation",
            "of": "env-brsu:bottle-frame",
            "with-respect-to": "sim-pickup:world-frame"
        },
        {
            "@id": "sim-pickup:bottle-pose", "@type": "Pose",
            "of": "env-brsu:bottle-frame",
            "with-respect-to": "sim-pickup:world-frame"
        },
        {
            "@id": "sim-pickup:bottle-pose-from-position-orientation",
            "@type": [
                "geom-sesame:PoseFromPositionOrientation", "PoseReference",
                "PositionReference", "OrientationReference", "PoseCoordinate"
            ],
            "of-pose": "sim-pickup:bottle-pose",
            "of-position": "sim-pickup:bottle-position",
            "of-orientation": "sim-pickup:bottle-orientation"
        },

        {
            "@id": "sim-pickup:battery-cyl-position", "@type": "Position",
            "of": "env-avl:cylindrical1-origin",
            "with-respect-to": "sim-pickup:world-frame-origin"
        },
        {
            "@id": "sim-pickup:battery-cyl-orientation", "@type": "Orientation",
            "of": "env-avl:cylindrical1-frame",
            "with-respect-to": "sim-pickup:world-frame"
        },
        {
            "@id": "sim-pickup:battery-cyl-pose", "@type": "Pose",
            "of": "env-avl:cylindrical1-frame",
            "with-respect-to": "sim-pickup:world-frame"
        },
        {
            "@id": "sim-pickup:battery-cyl-pose-from-position-orientation",
            "@type": [
                "geom-sesame:PoseFromPositionOrientation", "PoseReference",
                "PositionReference", "OrientationReference", "PoseCoordinate"
            ],
            "of-pose": "sim-pickup:battery-cyl-pose",
            "of-position": "sim-pickup:battery-cyl-position",
            "of-orientation": "sim-pickup:battery-cyl-orientation"
        }
    ]
}