{
    "@context": [
        "https://comp-rob2b.github.io/metamodels/geometry/structural-entities.json",
        {
            "avl": "https://hbrs-sesame.github.io/models/environments/avl/",
            "env": "https://hbrs-sesame.github.io/metamodels/environment#",
            "of-object": { "@id": "env:of-object", "@type": "@id" }
        }
    ],
    "@graph": [
        { "@id": "avl:cylindrical1-origin", "@type": "Point" },
        { "@id": "avl:cylindrical1-com", "@type": "Point" },
        {
            "@id": "avl:cylindrical1-frame", "@type": "Frame",
            "origin": "avl:cylindrical1-origin"
        },
        {
            "@id": "avl:cylindrical1-body", "@type": [ "RigidBody", "SimplicialComplex", "env:RigidObject" ],
            "of-object": "avl:cylindrical1",
            "simplices": [
                "avl:cylindrical1-origin",
                "avl:cylindrical1-frame",
                "avl:cylindrical1-com"
            ]
        },

        { "@id": "avl:pouch1-origin", "@type": "Point" },
        { "@id": "avl:pouch1-com", "@type": "Point" },
        {
            "@id": "avl:pouch1-frame", "@type": "Frame",
            "origin": "avl:pouch1-origin"
        },
        {
            "@id": "avl:pouch1-body", "@type": [ "RigidBody", "SimplicialComplex", "env:RigidObject" ],
            "of-object": "avl:pouch1",
            "simplices": [
                "avl:pouch1-origin",
                "avl:pouch1-frame",
                "avl:pouch1-com"
            ]
        },

        { "@id": "avl:prismatic-origin", "@type": "Point" },
        { "@id": "avl:prismatic-com", "@type": "Point" },
        {
            "@id": "avl:prismatic-frame", "@type": "Frame",
            "origin": "avl:prismatic-origin"
        },
        {
            "@id": "avl:prismatic-body", "@type": [ "RigidBody", "SimplicialComplex", "env:RigidObject" ],
            "of-object": "avl:prismatic",
            "simplices": [
                "avl:prismatic-origin",
                "avl:prismatic-frame",
                "avl:prismatic-com"
            ]
        },

        { "@id": "avl:tray-origin", "@type": "Point" },
        { "@id": "avl:tray-com", "@type": "Point" },
        {
            "@id": "avl:tray-frame", "@type": "Frame",
            "origin": "avl:tray-origin"
        },
        {
            "@id": "avl:tray-body", "@type": [ "RigidBody", "SimplicialComplex", "env:RigidObject" ],
            "of-object": "avl:tray",
            "simplices": [
                "avl:tray-origin",
                "avl:tray-frame",
                "avl:tray-com"
            ]
        }
    ]
}